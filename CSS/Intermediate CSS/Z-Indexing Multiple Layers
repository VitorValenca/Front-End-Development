#The largest z-index will position itself frontmost, while the smallest z-index will be at the backmost.
div {
 width: 100px;
 height: 100px;
 position: absolute;
}

.plum {
 background-color: plum;
 z-index: 38;
}

.coral {
 background-color: coral;
 top: 50px;
 z-index: 36;
}

.navy {
 background-color: navy;
 top: 75px;
 z-index: 37;
}
